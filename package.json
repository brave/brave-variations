{
  "name": "brave-variations",
  "version": "0.0.1",
  "description": "Brave Variations",
  "scripts": {
    "build": "npm run build:proto && npm run build:web && npm run build:tracker",
    "build:proto": "pbjs -t static-module --keep-case ./src/proto/variations_seed.proto ./src/proto/study.proto -o src/proto/generated/proto_bundle.js && pbts -o src/proto/generated/proto_bundle.d.ts src/proto/generated/proto_bundle.js",
    "build:tracker": "tsc -p src/finch_tracker/tsconfig.json",
    "build:web": "webpack -c src/web/webpack.config.js --mode=production",
    "clean": "tsx src/scripts/clean.ts",
    "lint": "tsx src/scripts/lint.ts",
    "serve:dev": "webpack serve -c src/web/webpack.config.js --hot --mode=development",
    "test": "jest --config=src/jest.config.js",
    "tracker": "node src/finch_tracker/build/finch_tracker/main.js",
    "typecheck": "tsc --noEmit",
    "typecheck:scripts": "tsc --noEmit --project src/scripts"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "12.1.0",
    "@jest/globals": "29.7.0",
    "@types/node": "20.14.7",
    "@types/react": "18.3.3",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "commander": "12.1.0",
    "css-loader": "5.2.7",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "39.1.1",
    "eslint-plugin-react": "7.34.3",
    "file-loader": "6.2.0",
    "jest": "29.7.0",
    "lint-staged": "15.2.7",
    "prettier": "3.3.2",
    "prettier-plugin-organize-imports": "3.2.4",
    "protobufjs": "7.3.2",
    "protobufjs-cli": "1.1.2",
    "style-loader": "3.3.4",
    "ts-jest": "29.1.5",
    "ts-loader": "9.5.1",
    "tsx": "4.15.7",
    "typescript": "5.5.2",
    "webpack": "5.92.1",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.15.2"
  },
  "type": "commonjs",
  "license": "MPL-2.0",
  "moduleResolution": "node",
  "dependencies": {
    "@types/react-dom": "^18.2.7",
    "eslint-plugin-licenses": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0"
  }
}
