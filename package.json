{
  "name": "brave-variations",
  "version": "0.0.1",
  "description": "Brave Variations",
  "scripts": {
    "build:proto": "tsx src/buildtools/generate_proto_ts.ts",
    "build:tracker": "tsc -p src/finch_tracker/tsconfig.json",
    "build:web": "webpack -c src/web/webpack.config.js --mode=production",
    "build": "npm run build:proto && npm run build:web && npm run build:tracker",
    "clean": "rm -rf ./src/proto/generated/proto_bundle* ./src/web/public/bundle/ ./src/finch_tracker/build/",
    "lint": "tsx src/lint.ts",
    "seed_tools": "tsx src/seed_tools/main.ts",
    "serve:dev": "webpack serve -c src/web/webpack.config.js --hot --mode=development",
    "test": "jest --config=src/jest.config.js",
    "tracker": "node src/finch_tracker/build/finch_tracker/main.js",
    "typecheck": "tsc --noEmit --project src"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/diff": "5.0.9",
    "@types/fs-extra": "11.0.4",
    "@types/jest": "29.5.12",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "css-loader": "^5.1.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-react": "^7.33.2",
    "execa": "8.0.1",
    "file-loader": "^6.2.0",
    "jest": "^29.7.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prettier-plugin-organize-imports": "3.2.4",
    "protobufjs": "7.2.6",
    "protobufjs-cli": "^1.1.2",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "tsx": "4.7.1",
    "typescript": "^5.2.2",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.15.1"
  },
  "type": "commonjs",
  "license": "MPL-2.0",
  "moduleResolution": "node",
  "dependencies": {
    "@commander-js/extra-typings": "^11.0.0",
    "@protobuf-ts/plugin": "2.9.4",
    "@protobuf-ts/runtime": "2.9.4",
    "@swc/core": "1.4.8",
    "@types/react-dom": "^18.2.7",
    "commander": "^11.0.0",
    "eslint-plugin-licenses": "^1.0.2",
    "fs-extra": "11.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0"
  }
}
