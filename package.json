{
  "name": "brave-variations",
  "version": "0.0.1",
  "description": "Brave Variations",
  "scripts": {
    "test": "jest --config src/jest.config.js",
    "lint": "eslint src --ext ts,tsx",
    "tracker": "node src/finch_tracker/build/finch_tracker/main.js",
    "clean": "rm -rf ./src/proto/generated/proto_bundle* src/web/public/bundle/ src/finch_tracker/build/",
    "build:proto": "pbjs -t static-module --keep-case ./src/proto/variations_seed.proto ./src/proto/study.proto -o src/proto/generated/proto_bundle.js && pbts -o src/proto/generated/proto_bundle.d.ts src/proto/generated/proto_bundle.js",
    "build:tracker": "tsc -p src/finch_tracker/tsconfig.json",
    "build:web": "webpack -c src/web/webpack.config.js --mode=production",
    "build": "npm run build:proto && npm run build:web && npm run build:tracker",
    "serve:dev": "webpack serve -c src/web/webpack.config.js --hot --mode=development"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "css-loader": "5.2.7",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard-with-typescript": "39.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "file-loader": "6.2.0",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "protobufjs": "7.2.5",
    "protobufjs-cli": "1.1.2",
    "style-loader": "3.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "typescript": "5.2.2",
    "webpack": "5.88.2",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.15.1"
  },
  "type": "commonjs",
  "moduleResolution": "node",
  "dependencies": {
    "@commander-js/extra-typings": "^11.0.0",
    "@types/react-dom": "^18.2.7",
    "commander": "^11.0.0",
    "eslint-plugin-licenses": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0"
  }
}
