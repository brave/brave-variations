{
  "name": "brave-variations",
  "version": "0.0.1",
  "description": "Brave Variations",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .js,.ts",
    "tracker": "node finch_tracker/build/finch_tracker/finch_tracker_main.js",
    "clean": "rm -rf ./proto/generated/proto_bundle* web/static/bundle.js finch_tracker/build/",
    "build:proto": "pbjs -t static-module --keep-case ./proto/variations_seed.proto ./proto/study.proto -o proto/generated/proto_bundle.js && pbts -o proto/generated/proto_bundle.d.ts proto/generated/proto_bundle.js",
    "build:tracker": "tsc -p finch_tracker/tsconfig.json",
    "build:web": "tsc -p web/tsconfig.json",
    "build:webpack": "webpack",
    "build": "npm run build:proto && npm run build:web && npm run build:webpack && npm run build:tracker"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "css-loader": "^5.1.3",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "file-loader": "^6.2.0",
    "prettier": "^3.0.2",
    "protobufjs": "^7.2.4",
    "protobufjs-cli": "^1.1.2",
    "style-loader": "^3.3.3",
    "typescript": "^5.2.2",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.5.0"
  },
  "type": "commonjs",
  "moduleResolution": "node",
  "dependencies": {
    "commander": "^11.0.0",
    "eslint-plugin-licenses": "^1.0.2",
    "vue": "^2.6.12"
  }
}
